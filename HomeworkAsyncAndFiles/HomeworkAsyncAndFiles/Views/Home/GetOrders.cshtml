@model IOrderedEnumerable<OrderModel>

<table class="table">
    <thead>
        <tr>
            <th>CustomerId</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
            <th>Age</th>
            <th>ProductNumber</th>
            <th>ProductName</th>
            <th>ProductWeight</th>
            <th>ProductPrice</th>
            <th>DateOfCreating</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.CustomerId</td>
                <td>@order.CustomersFirstName</td>
                <td>@order.CustomersLastName</td>
                <td>@order.CustomersEmail</td>
                <td>@order.CustomersAge</td>
                <td>@order.ProductNumber</td>
                <td>@order.ProductName</td>
                <td>@order.ProductWeight</td>
                <td>@order.ProductPrice</td>
                <td>@order.DateOfCreating</td>
            </tr>
        }
    </tbody>
</table>
